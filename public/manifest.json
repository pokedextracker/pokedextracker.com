{
  "$schema": "https://json.schemastore.org/web-manifest-combined.json",
  "id": "pokedextracker:main",
  "name": "Pok√©dex Tracker",
  "description": "A tool for tracking your Living Dex progress.",
  "display": "standalone",
  "start_url": "/profile",
  "theme_color": "#ffd924",
  "background_color": "#ffffff",
  "icons": [
    {
      "src": "/pokeball.svg",
      "sizes": "any",
      "type": "image/svg+xml"
    },
    {
      "src": "/favicon/android-chrome-36x36.png",
      "sizes": "36x36",
      "type": "image/png"
    },
    {
      "src": "/favicon/android-chrome-48x48.png",
      "sizes": "48x48",
      "type": "image/png"
    },
    {
      "src": "/favicon/android-chrome-72x72.png",
      "sizes": "72x72",
      "type": "image/png"
    },
    {
      "src": "/favicon/android-chrome-96x96.png",
      "sizes": "96x96",
      "type": "image/png"
    },
    {
      "src": "/favicon/android-chrome-144x144.png",
      "sizes": "144x144",
      "type": "image/png"
    },
    {
      "src": "/favicon/android-chrome-192x192.png",
      "sizes": "192x192",
      "type": "image/png"
    }
  ],
  "shortcuts": [
    {
      "name": "Profile",
      "url": "/profile",
      "description": "Your profile with a list of all your dexes."
    },
    {
      "name": "Account Settings",
      "url": "/account",
      "description": "Account settings that you can edit."
    }
  ]
}
